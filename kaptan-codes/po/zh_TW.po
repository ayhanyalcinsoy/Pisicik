msgid "Choose an image"
msgstr "選擇影像"

#: .tmp/ui_scrAvatar.py:260
msgid "Capture"
msgstr "捕捉"

#: .tmp/ui_scrAvatar.py:261
msgid "Recapture"
msgstr "再次捕捉"

